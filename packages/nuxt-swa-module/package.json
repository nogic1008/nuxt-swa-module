{
  "name": "nuxt-swa-module",
  "version": "0.0.1",
  "description": "Nuxt module for Azure Static Web Apps",
  "repository": "nogic1008/nuxt-swa-module",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
