    "prepare": "nuxi prepare && nuxt-module-build --stub"
  },
  "dependencies": {
    "@nuxt/kit": "^3.4.1"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@nuxt/module-builder": "^0.3.0",
